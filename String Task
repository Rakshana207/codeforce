#include<bits/stdc++.h>
using namespace std;
 int main(){
     string S,S1;
     cin>>S;
     int len=S.length();
     for(int i=0;i<len;i++)
     {
         if(S[i]=='a'|| S[i]=='e' || S[i]=='i'||S[i]=='o'||S[i]=='u'||S[i]=='y'||S[i]=='A'|| S[i]=='E'|| S[i]=='I'||S[i]=='O'||S[i]=='U'||S[i]=='Y')
         {
             continue;
         }
         else
         {
             S1+='.';
             S1+=tolower(S[i]);
         }
     }
     cout<<S1;
     return 0;
 }
